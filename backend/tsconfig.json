{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "typeRoots": ["./node_modules/@types", "./src/common/types"],
"types": ["node", "express"],
"skipLibCheck": true,
  },
  "exclude": ["dist", "node_modules", "e2e"]
}
